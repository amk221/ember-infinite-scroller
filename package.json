{
  "name": "@zestia/ember-simple-infinite-scroller",
  "version": "4.0.0",
  "description": "Simple infinite scroller component for Ember apps",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint": "yarn lint:js && yarn lint:hbs && yarn lint:css",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "lint:css": "stylelint --allow-empty-input '**/*.{css,scss}'",
    "prettier": "prettier '**/*.{js,json,css,scss,html,md}' --write",
    "start": "ember serve",
    "test": "ember test && yarn lint",
    "test:all": "ember try:each",
    "deploy": "ember build --environment production && ember github-pages:commit --message \"Deploy gh-pages from commit $(git rev-parse HEAD)\" && git push origin gh-pages:gh-pages"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zestia/ember-simple-infinite-scroller.git"
  },
  "engines": {
    "node": "8.* || >= 10.*"
  },
  "author": "Zestia",
  "license": "MIT",
  "devDependencies": {
    "@ember/optional-features": "0.7.0",
    "@ember/render-modifiers": "1.0.1",
    "@zestia/ember-template-lint-plugin": "2.0.4",
    "@zestia/eslint-config": "2.0.27",
    "@zestia/prettier-config": "0.0.5",
    "@zestia/stylelint-config": "2.0.21",
    "broccoli-asset-rev": "3.0.0",
    "ember-cli": "3.12.0",
    "ember-cli-dependency-checker": "3.2.0",
    "ember-cli-github-pages": "0.2.2",
    "ember-cli-htmlbars-inline-precompile": "3.0.0",
    "ember-cli-inject-live-reload": "2.0.1",
    "ember-cli-release-tag": "0.1.5",
    "ember-cli-sri": "2.1.1",
    "ember-cli-test-loader": "2.2.0",
    "ember-cli-uglify": "3.0.0",
    "ember-disable-prototype-extensions": "1.1.3",
    "ember-load-initializers": "2.1.0",
    "ember-maybe-import-regenerator": "0.1.6",
    "ember-qunit": "4.5.1",
    "ember-resolver": "5.2.1",
    "ember-source": "3.12.0",
    "ember-source-channel-url": "2.0.1",
    "ember-template-lint": "1.5.3",
    "ember-try": "1.2.1",
    "eslint": "6.3.0",
    "loader.js": "4.7.0",
    "prettier": "1.18.2",
    "qunit-dom": "0.9.0",
    "sass": "1.22.10",
    "stylelint": "10.1.0"
  },
  "dependencies": {
    "ember-angle-bracket-invocation-polyfill": "^2.0.2",
    "ember-cli-babel": "^7.11.0",
    "ember-cli-htmlbars": "^3.1.0",
    "ember-named-arguments-polyfill": "^1.0.0"
  },
  "keywords": [
    "ember-addon",
    "infinite",
    "scroll"
  ],
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://zestia.github.io/ember-simple-infinite-scroller"
  },
  "bugs": {
    "url": "https://github.com/zestia/ember-simple-infinite-scroller/issues"
  },
  "homepage": "https://github.com/zestia/ember-simple-infinite-scroller",
  "configPath": "tests/dummy/config"
}